{
   "name": "task",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "typeorm:init": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js init",
      "typeorm:migration:generate": "npx ts-node ./node_modules/typeorm/cli migration:generate -d src/migrations -n",
      "typeorm:migration:run": "npx ts-node ./node_modules/typeorm/cli migration:run",
      "migration:generate": "npx ts-node ./node_modules/typeorm/cli migration:generate --config src/infrastructure/configs/database.config",
      "migration:revert": "npx ts-node ./node_modules/typeorm/cli migration:revert --config src/infrastructure/configs/database.config",
      "migration:show": "npx ts-node ./node_modules/typeorm/cli migration:show --config src/infrastructure/configs/database.config",
      "migration:run": "npx ts-node ./node_modules/typeorm/cli migration:run --config src/infrastructure/configs/database.config",
      "typeorm:schema:sync": "npx ts-node ./node_modules/typeorm/cli schema:sync",
      "docker": "docker-compose -f docker/docker-compose.yml -p task up",
      "docker:mysql": "docker-compose -f docker/docker-compose.yml -p task exec db mysql -uroot -ppassword"
   },
   "dependencies": {
      "@apollo/gateway": "^0.45.0",
      "@badrap/result": "^0.2.8",
      "@hapi/joi": "^17.1.1",
      "@nestjs/common": "^8.2.4",
      "@nestjs/config": "^1.1.6",
      "@nestjs/core": "^8.2.4",
      "@nestjs/cqrs": "^8.0.1",
      "@nestjs/graphql": "^9.1.2",
      "@nestjs/microservices": "^8.2.4",
      "@nestjs/platform-express": "^8.2.4",
      "@nestjs/swagger": "^5.1.5",
      "@nestjs/typeorm": "^8.0.2",
      "apollo-server-express": "^3.6.1",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "dotenv": "^10.0.0",
      "env-var": "^7.1.1",
      "graphql": "^16.2.0",
      "i": "^0.3.7",
      "mysql": "^2.18.1",
      "nanoid": "^3.1.30",
      "nest-event": "^1.0.8",
      "nestjs-console": "^7.0.1",
      "npm": "^8.3.0",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.5.1",
      "swagger-ui-express": "^4.3.0",
      "ts-morph": "^13.0.2",
      "typeorm": "^0.2.41",
      "typeorm-seeding": "^1.6.1",
      "uuid": "^8.3.2",
      "validator": "^13.7.0",
      "ws": "^8.4.0"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.1.6",
      "@nestjs/schematics": "^8.0.5",
      "@nestjs/testing": "^8.2.4",
      "@types/express": "^4.17.13",
      "@types/faker": "^5.5.9",
      "@types/jest": "27.4.0",
      "@types/joi": "^17.2.2",
      "@types/node": "^17.0.7",
      "@types/supertest": "^2.0.11",
      "@types/uuid": "^8.3.3",
      "@types/ws": "^8.2.2",
      "@typescript-eslint/eslint-plugin": "5.8.1",
      "@typescript-eslint/parser": "5.8.1",
      "eslint": "8.6.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.4",
      "eslint-plugin-prettier": "^4.0.0",
      "eslint-plugin-simple-import-sort": "^7.0.0",
      "faker": "^5.5.3",
      "jest": "27.4.5",
      "jest-cucumber": "^3.0.1",
      "prettier": "^2.5.1",
      "supertest": "^6.1.6",
      "ts-jest": "27.1.2",
      "ts-loader": "^9.2.6",
      "ts-node": "10.4.0",
      "tsconfig-paths": "^3.12.0",
      "typescript": "^4.5.4"
   },
   "husky": {
      "hooks": {
         "pre-push": "npm run test:all && npm run lint && npm run format"
      }
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "<rootDir>/src/**/*.ts"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node",
      "moduleNameMapper": {
         "^@/(.*)$": "<rootDir>/src/$1"
      },
      "roots": [
         "<rootDir>/src"
      ],
      "testPathIgnorePatterns": [
         "<rootDir>/node_modules/', '<rootDir>/coverage/', '<rootDir>/lib/"
      ],
      "collectCoverage": false,
      "cacheDirectory": "<rootDir>/coverage/cache"
   }
}